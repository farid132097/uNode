 206               	;used registers          : R18, R19                                                            
 207               	;arg registers           : None                                                                
 208               	;return registers        : None                                                                
 209               	;unsafe access registers : R18, R19                                                            
 210               	.macro  KER_TIMER_INIT                                    ;1.25/1.50uS @8MHz    (10/12 clocks) 
 211               	        #ifdef KER_TIMER0_AS_TICK_SRC                                                          
 212               			;Timer0 overflow vect is used as tick source      ;total 1.5uS @8MHz      ( 12 clocks) 
 213               	        LDS   R18		         , KerBase+OFB_PRS        ;load prescaler         (  2 clocks) 
 214               			LDS   R19		         , KerBase+OFB_RLD        ;load reload val        (  2 clocks) 
 215               			STS   SRTCCR0B           , R18                    ;prescaler-> TCCR0B     (  2 clocks) 
 216               			STS   SRTCNT0            , R19                    ;reload val-> TCNT0     (  2 clocks) 
 217               			LDI   R18                , 0x01                   ;bit mask TOIE0         (  1 clock ) 
 218               			STS   SRTIMSK0           , R18                    ;set to TIMSK0          (  2 clocks) 
 219               			SEI                                               ;force enable interrupt (  1 clock ) 
 220               			#endif                                                                                 
 221               			                                                                                       
 222               			#ifdef KER_WDT_AS_TICK_SRC                                                             
 223               			;watchdog timer timeout is used as tick source    ;total 1.5uS @8MHz      ( 10 clocks) 
 224               	        WDR                                               ;reset wdt              (  1 clock ) 
 225               			LDS   R18                , SRWDTCSR               ;copy WDTCSR            (  2 clocks) 
 226               			ORI   R18                , 0x18                   ;set WDCE,WDE           (  1 clock ) 
 227               			STS   SRWDTCSR           , R18                    ;set val                (  2 clocks) 
 228               			                                                                                       
 229               			#ifdef KER_WDT_TICK_16MS                                                               
 230               			LDI   R18                , 0x40                   ;WDIE                   (  1 clock ) 
 231               			#endif                                                                                 
 232               			                                                                                       
 233               	        #ifdef KER_WDT_TICK_32MS                                                               
 234               			LDI   R18                , 0x41                   ;WDIE, WDPS0            (  1 clock ) 
 235               			#endif                                                                                 
 236               			                                                                                       
 237               			#ifdef KER_WDT_TICK_64MS                                                               
 238               			LDI   R18                , 0x42                   ;WDIE, WDPS1            (  1 clock ) 
 239               			#endif                                                                                 
 240               			                                                                                       
 241               			#ifdef KER_WDT_TICK_125MS                                                              
 242               			LDI   R18                , 0x43                   ;WDIE, WDPS1, WDPS2     (  1 clock ) 
 243               			#endif                                                                                 
 244               			                                                                                       
 245               			#ifdef KER_WDT_TICK_250MS                                                              
 246               			LDI   R18                , 0x44                   ;WDIE, WDPS1, WDPS2     (  1 clock ) 
 247               			#endif                                                                                 
 248               			                                                                                       
 249               			#ifdef KER_WDT_TICK_500MS                                                              
 250               			LDI   R18                , 0x45                   ;WDIE, WDPS1, WDPS2     (  1 clock ) 
 251               			#endif                                                                                 
 252               			                                                                                       
 253               			#ifdef KER_WDT_TICK_1000MS                                                             
 254               			LDI   R18                , 0x46                   ;WDIE, WDPS1, WDPS2     (  1 clock ) 
 255               			#endif                                                                                 
 256               			                                                                                       
 257               			STS   SRWDTCSR           , R18                    ;set val                (  2 clocks) 
 258               			SEI                                               ;force enable interrupt (  1 clock ) 
 259               			#endif                                                                                 
 260               	                                                                                               
 261               			#ifdef KER_TOSC_AS_TICK_SRC                                                            
 262               			;timer2 asynchornus mode is used as tick source   ;total 1.5uS @8MHz      ( 57 clocks) 
 263               			LDI   R18                , 0x00                   ;clear interrupts       (  1 clock ) 
 264               			STS   SRTIMSK2           , R18                    ;set val to TIMSK2      (  2 clocks) 
 265               			LDI   R18                , 0x20                   ;set AS2 bit            (  1 clock ) 
 266               			STS   SRASSR             , R18                    ;set val to ASSR        (  2 clocks) 
 267               			#ifdef KER_TOSC_TICK_1MS                                                               
 268               			LDI   R18                , 0x20                   ;1000Hz->clk/1/32       (  1 clock ) 
 269               			#endif                                                                                 
 270               			#ifdef KER_TOSC_TICK_10MS                                                              
 271               			LDI   R18                , 0x29                   ;100Hz->clk/8/41        (  1 clock ) 
 272               			#endif                                                                                 
 273               			#ifdef KER_TOSC_TICK_50MS                                                              
 274               			LDI   R18                , 0xCC                   ;20Hz->clk/8/204        (  1 clock ) 
 275               			#endif                                                                                 
 276               			#ifdef KER_TOSC_TICK_100MS                                                             
 277               			LDI   R18                , 0x66                   ;10Hz->clk/32/102       (  1 clock ) 
 278               			#endif                                                                                 
 279               			#ifdef KER_TOSC_TICK_250MS                                                             
 280               			LDI   R18                , 0x80                   ;4Hz->clk/64/128        (  1 clock ) 
 281               			#endif                                                                                 
 282               			#ifdef KER_TOSC_TICK_500MS                                                             
 283               			LDI   R18                , 0x80                   ;2Hz->clk/128/128       (  1 clock ) 
 284               			#endif                                                                                 
 285               			#ifdef KER_TOSC_TICK_1000MS                                                            
 286               			LDI   R18                , 0x80                   ;1Hz->clk/256/128       (  1 clock ) 
 287               			#endif                                                                                 
 288               			STS   SROCR2A            , R18                    ;set val to OCR2A       (  2 clocks) 
 289               			LDI   R18                , 0x00                   ;set val to reg         (  1 clock ) 
 290               			STS   SROCR2B            , R18                    ;set val to OCR2B       (  2 clocks) 
 291               			LDI   R18                , 0x02                   ;set WGM mode           (  1 clock ) 
 292               			STS   SRTCCR2A           , R18                    ;set val to TCCR2A      (  2 clocks) 
 293               			#ifdef KER_TOSC_TICK_1MS                                                               
 294               			LDI   R18                , 0x01                   ;1000Hz->clk/1/32       (  1 clock ) 
 295               			#endif                                                                                 
 296               			#ifdef KER_TOSC_TICK_10MS                                                              
 297               			LDI   R18                , 0x02                   ;100Hz->clk/8/41        (  1 clock ) 
 298               			#endif                                                                                 
 299               			#ifdef KER_TOSC_TICK_50MS                                                              
 300               			LDI   R18                , 0x02                   ;20Hz->clk/8/204        (  1 clock ) 
 301               			#endif                                                                                 
 302               			#ifdef KER_TOSC_TICK_100MS                                                             
 303               			LDI   R18                , 0x03                   ;10Hz->clk/32/102       (  1 clock ) 
 304               			#endif                                                                                 
 305               			#ifdef KER_TOSC_TICK_250MS                                                             
 306               			LDI   R18                , 0x04                   ;4Hz->clk/64/128        (  1 clock ) 
 307               			#endif                                                                                 
 308               			#ifdef KER_TOSC_TICK_500MS                                                             
 309               			LDI   R18                , 0x05                   ;2Hz->clk/128/128       (  1 clock ) 
 310               			#endif                                                                                 
 311               			#ifdef KER_TOSC_TICK_1000MS                                                            
 312               			LDI   R18                , 0x06                   ;1Hz->clk/256/128       (  1 clock ) 
 313               			#endif                                                                                 
 314               			STS   SRTCCR2B           , R18                    ;set val to TCCR2B      (  2 clocks) 
 315               			LDI   R18                , 0x00                   ;clear reg              (  1 clock ) 
 316               			STS   SRTCNT2            , R18                    ;set val to TCNT2       (  2 clocks) 
 317               		_KER_TC2_AUB\@:                                                                            
 318               		    LDS   R18                , SRASSR                 ;load ASSR              (  2 clocks) 
 319               	        ANDI  R18                , 0x02                   ;check bit TCR2AUB      (  1 clock ) 
 320               			BRNE  _KER_TC2_AUB\@                              ;wait until AUB cleared (  2 clocks) 
 321               		_KER_TC2_BUB\@:                                                                            
 322               		    LDS   R18                , SRASSR                 ;load ASSR              (  2 clocks) 
 323               	        ANDI  R18                , 0x01                   ;check bit TCR2BUB      (  1 clock ) 
 324               			BRNE  _KER_TC2_BUB\@                              ;wait until BUB cleared (  2 clocks) 
 325               		_KER_OC2_AUB\@:                                                                            
 326               		    LDS   R18                , SRASSR                 ;load ASSR              (  2 clocks) 
 327               	        ANDI  R18                , 0x08                   ;check bit OR2AUB       (  1 clock ) 
 328               			BRNE  _KER_OC2_AUB\@                              ;wait until AUB cleared (  2 clocks) 
 329               		_KER_OC2_BUB\@:                                                                            
 330               		    LDS   R18                , SRASSR                 ;load ASSR              (  2 clocks) 
 331               	        ANDI  R18                , 0x04                   ;check bit OCR2BUB      (  1 clock ) 
 332               			BRNE  _KER_OC2_BUB\@                              ;wait until BUB cleared (  2 clocks) 
 333               		_KER_TC2_UB\@:                                                                             
 334               		    LDS   R18                , SRASSR                 ;load ASSR              (  2 clocks) 
 335               	        ANDI  R18                , 0x10                   ;check bit TCNT2UB      (  1 clock ) 
 336               			BRNE  _KER_TC2_UB\@                               ;wait until BUB cleared (  2 clocks) 
 337               		_KER_TC2_TOV2\@:                                                                           
 338               		    LDS   R18                , SRTIFR2                ;load TIFR2             (  2 clocks) 
 339               	        ANDI  R18                , 0x01                   ;check bit TOV2         (  1 clock ) 
 340               	        BREQ  _KER_TC2_OCF2A\@                            ;bit cleared, jump next (  2 clocks) 
 341               			LDI   R18                , 0x01                   ;set bit                (  1 clock ) 
 342               			STS   SRTIFR2            , R18                    ;set val                (  2 clocks) 
 343               	    _KER_TC2_OCF2A\@:                                                                          
 344               		    LDS   R18                , SRTIFR2                ;load TIFR2             (  2 clocks) 
 345               	        ANDI  R18                , 0x02                   ;check bit OCF2A        (  1 clock ) 
 346               	        BREQ  _KER_TC2_OCF2B\@                            ;bit cleared, jump next (  2 clocks) 
 347               			LDI   R18                , 0x02                   ;set bit                (  1 clock ) 
 348               			STS   SRTIFR2            , R18                    ;set val                (  2 clocks) 
 349               		_KER_TC2_OCF2B\@:                                                                          
 350               		    LDS   R18                , SRTIFR2                ;load TIFR2             (  2 clocks) 
 351               	        ANDI  R18                , 0x04                   ;check bit OCF2B        (  1 clock ) 
 352               	        BREQ  _KER_TC2_INTEN\@                            ;bit cleared, jump next (  2 clocks) 
 353               			LDI   R18                , 0x04                   ;set bit                (  1 clock ) 
 354               			STS   SRTIFR2            , R18                    ;set val                (  2 clocks) 
 355               	    _KER_TC2_INTEN\@:                                                                          
 356               			LDI   R18                , 0x02                   ;set TOIE2 bit          (  1 clock ) 
 357               			STS   SRTIMSK2           , R18                    ;set val to TIMSK2      (  2 clocks) 
 358               	        SEI                                               ;force enable interrupt (  1 clock ) 
 359               			#endif                                                                                 
