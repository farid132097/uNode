 924               	.macro  KER_RUN_SCHEDULER                                 ;total 13.25uS @8MHz    (106 clocks) 
 925               			LDI    R18               , 0xFF                   ;set 0xff               (  1 clock ) 
 926               			STS    KerBase+OFB_LPR   , R18                    ;lowest priority        (  2 clocks) 
 927               			LDI    R18               , 0x00                   ;start from 0           (  1 clock ) 
 928               			STS    KerBase+OFB_PTID  , R18                    ;highest prio tid=0     (  2 clocks) 
 929               			MOV    R21               , R24                    ;copy sch mode          (  1 clock ) 
 930               		_KER_SCH_LOOP\@:                                                                           
 931               		    ;store task id to run from KER_DEC_SLP_TIMEOUT                                         
 932               			STS    KerBase+OFB_TID   , R18                    ;store task id          (  2 clocks) 
 933               	        ;sleep time decrement, update ready/blocked status                                     
 934               			MOV    R24               , R21                    ;restore sch mode       (  1 clock ) 
 935               			KER_SLP_TIME_MGNT                                 ;update return vars     ( 52 clocks) 
 936               	        CPI    R24               , TASK_READY             ;compare                (  1 clock ) 
 937               	        BREQ   _KER_CALC_PRIO\@                           ;calc priority if ready (  2 clocks) 
 938               			CPI    R24               , TASK_CONS_LAT          ;compare                (  1 clock ) 
 939               	        BREQ   _KER_CALC_PRIO\@                           ;calc priority if c_lat (  2 clocks) 
 940               	        RJMP   _KER_SCH_NEXT\@                            ;skip if !ready|c_lat   (  2 clocks) 
 941               		_KER_CALC_PRIO\@:                                                                          
 942               			KER_CURR_TASK_PRIO                                ;get task prio ->R24    (  9 clocks) 
 943               	        ;compare current task priority with lowest priority found so far                       
 944               			LDS    R18               , KerBase+OFB_LPR        ;load lowest priority   (  2 clocks) 
 945               			CP     R24               , R18                    ;compare                (  1 clock ) 
 946               			BRSH   _KER_SCH_NEXT\@                            ;if prio>=lowest prio   (  2 clocks) 
 947               			;found new lowest priority                                                             
 948               			STS    KerBase+OFB_LPR   , R24                    ;save lowest priority   (  2 clocks) 
 949               			LDS    R18               , KerBase+OFB_TID        ;load task id           (  2 clocks) 
 950               			STS    KerBase+OFB_PTID  , R18                    ;save lowest priority   (  2 clocks) 
 951               	                                                                                               
 952               	    _KER_SCH_NEXT\@:                                                                           
 953               		    LDS    R18               , KerBase+OFB_TID        ;load task id           (  2 clocks) 
 954               			INC    R18                                        ;increment by 1         (  1 clock ) 
 955               			LDS    R19               , KerBase+OFB_NTSK       ;load ntask             (  2 clocks) 
 956               			CP     R18               , R19                    ;compare with ntask     (  2 clocks) 
 957               			BRSH   _KER_SCH_EXIT\@                            ;if task_id>=ntask      (  2 clocks) 
 958               			RJMP   _KER_SCH_LOOP\@                            ;jump to entry          (  2 clocks) 
 959               		_KER_SCH_EXIT\@:                                                                           
 960               	        LDS    R18               , KerBase+OFB_PTID       ;load high prio task id (  2 clocks) 
 961               			STS    KerBase+OFB_TID   , R18                    ;for test only          (  2 clocks) 
 962               	.endm                                                                                          
