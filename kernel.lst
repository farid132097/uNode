 854               	.macro  KER_SLP_TIME_MGNT                                 ;total 6.50uS @8MHz     ( 52 clocks) 
 855               			LDI   ZL                 , lo8(KerSchSlp)         ;fetch base pos low     (  1 clock ) 
 856               			LDI   ZH                 , hi8(KerSchSlp)         ;fetch base pos high    (  1 clock ) 
 857               			KER_CALC_ADDR_OFF_WORD                            ;calc offset            (  6 clocks) 
 858               			;fetch current value from ram, if val=0, skip decrement                                
 859               	        LDD   R18                , Z+0                    ;load val low byte      (  2 clocks) 
 860               			LDD   R19                , Z+1                    ;load val high byte     (  2 clocks) 
 861               			MOV   R20                , R18                    ;copy                   (  1 clock ) 
 862               			OR    R20                , R19                    ;or high & low bytes    (  1 clock ) 
 863               			BREQ  _VAL_NULL\@                                 ;val=0, save sts        (  2 clocks) 
 864               	        CPI   R24                , SCH_MODE_THREAD        ;if arg=1, thread mode  (  1 clock ) 
 865               			BREQ  _VAL_NOT_NULL\@                             ;no need to dec val     (  2 clocks) 
 866               			;R19:R18 contains 16 bit sleep timer val, decrease val by 1                            
 867               			LDI   R20                , 0x01                   ;set val 1              (  1 clock ) 
 868               	        SUB   R18                , R20                    ;subtract low byte      (  1 clock ) 
 869               			LDI   R20                , 0x00                   ;clear                  (  1 clock ) 
 870               			SBC   R19                , R20                    ;subtract carry if any  (  1 clock ) 
 871               			;store new value                                                                       
 872               			STD   Z+0                , R18                    ;store low byte         (  2 clocks) 
 873               			STD   Z+1                , R19                    ;store low byte         (  2 clocks) 
 874               			MOV   R20                , R18                    ;copy                   (  1 clock ) 
 875               			OR    R20                , R19                    ;or high & low bytes    (  1 clock ) 
 876               			BRNE  _VAL_NOT_NULL\@                             ;val!=0                 (  2 clocks) 
 877               		_VAL_NULL\@:                                                                               
 878               		    ;find ram address for status                                                           
 879               		    LDI   ZL                 , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 880               			LDI   ZH                 , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 881               	        KER_CALC_ADDR_OFF_BYTES                           ;calc offset            (  5 clocks) 
 882               			;update flag as task is ready                                                          
 883               			LDI   R24                , TASK_READY             ;set TASK_READY         (  1 clock ) 
 884               			ST    Z                  , R24                    ;update flag            (  2 clocks) 
 885               			RJMP  _EXIT_SLP_TIME\@                            ;jump to exit           (  2 clocks) 
 886               	    _VAL_NOT_NULL\@:                                                                           
 887               		    LDI   ZL                 , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 888               			LDI   ZH                 , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 889               	        KER_CALC_ADDR_OFF_BYTES                           ;calc offset            (  5 clocks) 
 890               		    LD    R24                , Z                      ;return sts             (  2 clocks) 
 891               	    _EXIT_SLP_TIME\@:                                                                          
