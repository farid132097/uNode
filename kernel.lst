 923               	.macro  KER_RUN_SCHEDULER                                 ;total 13.25uS @8MHz    (106 clocks) 
 924               			LDI    R18               , 0xFF                   ;set 0xff               (  1 clock ) 
 925               			STS    KerBase+OFB_LPR   , R18                    ;lowest priority        (  2 clocks) 
 926               			LDI    R18               , 0x00                   ;start from 0           (  1 clock ) 
 927               			STS    KerBase+OFB_PTID  , R18                    ;highest prio tid=0     (  2 clocks) 
 928               			MOV    R21               , R24                    ;copy sch mode          (  1 clock ) 
 929               		_KER_SCH_LOOP\@:                                                                           
 930               		    ;store task id to run from KER_DEC_SLP_TIMEOUT                                         
 931               			STS    KerBase+OFB_TID   , R18                    ;store task id          (  2 clocks) 
 932               	        ;sleep time decrement, update ready/blocked status                                     
 933               			MOV    R24               , R21                    ;restore sch mode       (  1 clock ) 
 934               			KER_SLP_TIME_MGNT                                 ;update return vars     ( 52 clocks) 
 935               	        CPI    R24               , TASK_READY             ;compare                (  1 clock ) 
 936               	        BREQ   _KER_CALC_PRIO\@                           ;calc priority if ready (  2 clocks) 
 937               			CPI    R24               , TASK_CONS_LAT          ;compare                (  1 clock ) 
 938               	        BREQ   _KER_CALC_PRIO\@                           ;calc priority if c_lat (  2 clocks) 
 939               	        RJMP   _KER_SCH_NEXT\@                            ;skip if !ready|c_lat   (  2 clocks) 
 940               		_KER_CALC_PRIO\@:                                                                          
 941               			KER_CURR_TASK_PRIO                                ;get task prio ->R24    (  9 clocks) 
 942               	        ;compare current task priority with lowest priority found so far                       
 943               			LDS    R18               , KerBase+OFB_LPR        ;load lowest priority   (  2 clocks) 
 944               			CP     R24               , R18                    ;compare                (  1 clock ) 
 945               			BRSH   _KER_SCH_NEXT\@                            ;if prio>=lowest prio   (  2 clocks) 
 946               			;found new lowest priority                                                             
 947               			STS    KerBase+OFB_LPR   , R24                    ;save lowest priority   (  2 clocks) 
 948               			LDS    R18               , KerBase+OFB_TID        ;load task id           (  2 clocks) 
 949               			STS    KerBase+OFB_PTID  , R18                    ;save lowest priority   (  2 clocks) 
 950               	                                                                                               
 951               	    _KER_SCH_NEXT\@:                                                                           
 952               		    LDS    R18               , KerBase+OFB_TID        ;load task id           (  2 clocks) 
 953               			INC    R18                                        ;increment by 1         (  1 clock ) 
 954               			LDS    R19               , KerBase+OFB_NTSK       ;load ntask             (  2 clocks) 
 955               			CP     R18               , R19                    ;compare with ntask     (  2 clocks) 
 956               			BRSH   _KER_SCH_EXIT\@                            ;if task_id>=ntask      (  2 clocks) 
 957               			RJMP   _KER_SCH_LOOP\@                            ;jump to entry          (  2 clocks) 
 958               		_KER_SCH_EXIT\@:                                                                           
 959               	        LDS    R18               , KerBase+OFB_PTID       ;load high prio task id (  2 clocks) 
 960               			STS    KerBase+OFB_TID   , R18                    ;for test only          (  2 clocks) 
