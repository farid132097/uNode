 853               	.macro  KER_SLP_TIME_MGNT                                 ;total 6.50uS @8MHz     ( 52 clocks) 
 854               			LDI   ZL                 , lo8(KerSchSlp)         ;fetch base pos low     (  1 clock ) 
 855               			LDI   ZH                 , hi8(KerSchSlp)         ;fetch base pos high    (  1 clock ) 
 856               			KER_CALC_ADDR_OFF_WORD                            ;calc offset            (  6 clocks) 
 857               			;fetch current value from ram, if val=0, skip decrement                                
 858               	        LDD   R18                , Z+0                    ;load val low byte      (  2 clocks) 
 859               			LDD   R19                , Z+1                    ;load val high byte     (  2 clocks) 
 860               			MOV   R20                , R18                    ;copy                   (  1 clock ) 
 861               			OR    R20                , R19                    ;or high & low bytes    (  1 clock ) 
 862               			BREQ  _VAL_NULL\@                                 ;val=0, save sts        (  2 clocks) 
 863               	        CPI   R24                , SCH_MODE_THREAD        ;if arg=1, thread mode  (  1 clock ) 
 864               			BREQ  _VAL_NOT_NULL\@                             ;no need to dec val     (  2 clocks) 
 865               			;R19:R18 contains 16 bit sleep timer val, decrease val by 1                            
 866               			LDI   R20                , 0x01                   ;set val 1              (  1 clock ) 
 867               	        SUB   R18                , R20                    ;subtract low byte      (  1 clock ) 
 868               			LDI   R20                , 0x00                   ;clear                  (  1 clock ) 
 869               			SBC   R19                , R20                    ;subtract carry if any  (  1 clock ) 
 870               			;store new value                                                                       
 871               			STD   Z+0                , R18                    ;store low byte         (  2 clocks) 
 872               			STD   Z+1                , R19                    ;store low byte         (  2 clocks) 
 873               			MOV   R20                , R18                    ;copy                   (  1 clock ) 
 874               			OR    R20                , R19                    ;or high & low bytes    (  1 clock ) 
 875               			BRNE  _VAL_NOT_NULL\@                             ;val!=0                 (  2 clocks) 
 876               		_VAL_NULL\@:                                                                               
 877               		    ;find ram address for status                                                           
 878               		    LDI   ZL                 , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 879               			LDI   ZH                 , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 880               	        KER_CALC_ADDR_OFF_BYTES                           ;calc offset            (  5 clocks) 
 881               			;update flag as task is ready                                                          
 882               			LDI   R24                , TASK_READY             ;set TASK_READY         (  1 clock ) 
 883               			ST    Z                  , R24                    ;update flag            (  2 clocks) 
 884               			RJMP  _EXIT_SLP_TIME\@                            ;jump to exit           (  2 clocks) 
 885               	    _VAL_NOT_NULL\@:                                                                           
 886               		    LDI   ZL                 , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 887               			LDI   ZH                 , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 888               	        KER_CALC_ADDR_OFF_BYTES                           ;calc offset            (  5 clocks) 
 889               		    LD    R24                , Z                      ;return sts             (  2 clocks) 
 890               	    _EXIT_SLP_TIME\@:                                                                          
